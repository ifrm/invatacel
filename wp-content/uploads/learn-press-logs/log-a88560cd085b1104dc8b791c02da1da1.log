				FROM wp_posts c
				INNER JOIN wp_postmeta cm ON cm.post_id = c.ID AND cm.meta_key = '_lp_payment' AND cm.meta_value = 'yes'
				WHERE 1
				AND c.post_status = 'publish' AND c.post_type = 'lp_course'
				GROUP BY d
				HAVING d BETWEEN '2016-04-04' AND '2016-04-10'
				ORDER BY d ASC
			
